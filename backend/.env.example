# Copy this file to .env in this directory and edit the values.
# The backend loads .env from its current working directory.

# Path to the SQLite database file (default: ./pocketratings.db)
DB_PATH=./pocketratings.db

# JWT signing secret (required). Use a long random string in production.
JWT_SECRET=your-secret-here

# Address and port the API server binds to (default: 127.0.0.1:3099)
BIND=127.0.0.1:3099
